{
  "name": "uikit",
  "version": "0.1.0",
  "description": "vue3,vue通用组件库；ins-design；foxDesign",
  "author": "guojikun",
  "scripts": {
    "serve": "npm run serve:lib && npm run serve:docs",
    "build": "vue-cli-service build --modern --dest docs",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:docs": "npx vuepress build docs",
    "build:lib": "vue-cli-service build --dest ui --target lib --name ignis-ui ./src/index.ts",
    "serve:docs": "npx vuepress dev ./docs",
    "serve:lib": "vue-cli-service build --dest ui --target lib --name ignis-ui ./src/index.js",
    "serve:test": "vue-cli-service serve"
  },
  "main": "ui/ins-design.umd.js",
  "files": [
    "ui",
    "style",
    "src",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "async-validator": "^3.4.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0-0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-plugin-unit-jest": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  },
  "keywords": [
    "vue3",
    "vue",
    "component",
    "components",
    "ins-ui",
    "insUI",
    "ins-ui",
    "ui",
    "uikit",
    "view"
  ],
  "license": "MIT"
}
